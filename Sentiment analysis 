    "def analyze_audio_emotion(audio_features):\n",
    "    avg_pitch = audio_features['avg_pitch']\n",
    "\n",
    "    if avg_pitch > 200:  # High pitch\n",
    "        return \"Stress or Anger\"\n",
    "    elif avg_pitch < 150:  # Low pitch\n",
    "        return \"Calm or Neutral\"\n",
    "    else:\n",
    "        return \"Neutral\""
   ]
